@model List<Pokemon>
<h1>lista de cuidades pokemones</h1>
if(Model.Count == 0){
    <div class="alert alert-warning text-center">Aun no hay cuidades</div>
}else{
    <ol>
    @foreach (var p in Model)
    {
        <li class="d-flex">
            <div><img scr="@p.Foto"></div>
            <div>@p.Nombre</div>
        </li>
        
    }
    </ol>
}

